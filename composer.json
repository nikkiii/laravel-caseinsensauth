{
  "name": "nikkiii/caseinsensauth",
  "description": "The Illuminate Auth package with modifications for case insensitive auth.",
  "license": "MIT",
  "authors": [
    {
      "name": "Taylor Otwell",
      "email": "taylorotwell@gmail.com"
    },
    {
      "name": "Nikki",
      "email": "nospam@nikkii.us"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/contracts": "5.*",
    "illuminate/http": "5.*",
    "illuminate/session": "5.*",
    "illuminate/support": "5.*",
    "nesbot/carbon": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Nikkii\\CaseInsensAuth\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.1-dev"
    }
  },
  "suggest": {
    "illuminate/console": "Required to use the auth:clear-resets command (5.*)."
  },
  "minimum-stability": "dev"
}
